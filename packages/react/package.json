  "version": "0.1.0",
  "files": [
    "dist",
    "dist-esm"
  ],
  "main": "./dist/index.js",
  "module": "./dist-esm/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build:cjs": "yarn tsc",
    "build:esm": "yarn tsc -m es6 --outDir dist-esm",
    "build": "run-p build:*",
    "watch:cjs": "yarn tsc --watch",
    "watch:esm": "yarn tsc -m es6 --watch --outDir dist-esm",
    "watch": "run-p watch:*",
    "storybook": "yarn start-storybook",
    "storybook:build": "yarn build-storybook",
    "test": "yarn jest",
    "test:watch": "yarn jest --watch --bail",
    "prepublishOnly": "yarn run build",
    "release": "npm publish --access public"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "@dimension/core": "0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/react": "^6.0.0-alpha.13",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.19",
    "@types/testing-library__react": "^9.1.2",
    "babel-loader": "^8.0.6",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.12.0",
    "react-popper": "^1.3.7",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5"
  },
  "license": "MIT"
}
